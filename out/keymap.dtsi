        label = "PROFILE_BT_1";
        #binding-cells = <0>;
        tapping-term-ms = <200>;
        bindings = <&profile_bt_select_1>, <&bt BT_DISC 1>;
    };
    profile_bt_2: profile_bt_2 {
        compatible = "zmk,behavior-tap-dance";
        label = "PROFILE_BT_2";
        #binding-cells = <0>;
        tapping-term-ms = <200>;
        bindings = <&profile_bt_select_2>, <&bt BT_DISC 2>;
    };
    profile_bt_3: profile_bt_3 {
        compatible = "zmk,behavior-tap-dance";
