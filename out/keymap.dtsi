        #binding-cells = <0>;
        tapping-term-ms = <200>;
        bindings = <&bt_select_3>, <&bt BT_DISC 3>;
    };
#endif

#ifdef BT_DISC_CMD
    profile_bt_0: profile_bt_0 {
        compatible = "zmk,behavior-tap-dance";
        label = "PROFILE_BT_0";
        #binding-cells = <0>;
